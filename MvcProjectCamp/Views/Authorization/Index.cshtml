@using EntityLayer.Concreate
@model List<Admin>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Admin</th>
        <th>Yetki</th>
        <th>Pasif Yap</th>
        <th>Yetkisini Değiştir</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AdminID</th>
            <td>@item.AdminName</td>
            <td>@item.AdminRole</td>
            <td><a href="/Authorization/DeleteAdmin/@item.AdminID" class="btn btn-danger">Pasif </a></td>
            <td><a href="/Authorization/EditAdmin/@item.AdminID" class="btn btn-warning">Yetkisini Değiştir</a></td>
          
        </tr>
    }

</table>
<a href="/Authorization/AddAdmin" class="btn btn-info">Yeni Admin</a>